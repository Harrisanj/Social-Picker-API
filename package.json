{
  "name": "social-picker-api",
  "version": "2.3.1",
  "description": "Extract media from various posting platfroms like Twitter, Reddit, Pixiv, Youtube, Osnova and many others",
  "main": "social-picker-api.js",
  "type": "module",
  "scripts": {
    "production": "pm2 start config/pm2.production.json",
    "dev": "nodemon social-picker-service.js --config config/nodemon.dev.json",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serguun42/social-picker-api.git"
  },
  "keywords": [
    "instagram",
    "parser",
    "twitter",
    "reddit",
    "ffmpeg",
    "telegram-bot",
    "youtube-dl",
    "telegraf",
    "pixiv",
    "dtf",
    "ugoira",
    "tjournal",
    "twitter-api-v2",
    "yt-dlp",
    "osnova",
    "kemono-party",
    "tumblr",
    "joyreactor"
  ],
  "author": "serguun42",
  "license": "BSL-1.0 License",
  "dependencies": {
    "ffmpeg": "0.0.4",
    "jszip": "3.10.1",
    "node-fetch": "3.2.10",
    "node-html-parser": "6.1.1",
    "pm2": "5.2.2",
    "tumblr.js": "3.0.0",
    "twitter-lite": "1.1.0",
    "yt-dlp-wrap": "2.3.11"
  },
  "devDependencies": {
    "@types/node": "18.11.0",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  }
}
